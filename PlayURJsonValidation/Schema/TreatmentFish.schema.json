{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "TreatmentFishList",
  "description": "List of categories used for the Systemic Autoimmune Disease worksheet",
  "type": "object",
  "properties": {
    "Fish": {
      "type": "array",
      "items": {
        "type": "object",
        "title": "TreatmentFish",
        "properties": {
          "FishType": {
            "type": "integer",
            "minimum": 1
          },
          "SizeMin": {
            "type": "string"
          },
          "SizeMax": {
            "type": "integer"
          },
          "MinBagSize": {
            "type": "integer"
          },
          "BagLimit": {
            "type": "integer"
          }
        },
        "required": [
          "FishType",
          "SizeMin",
          "SizeMax"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": ["Fish"],
  "additionalProperties": false
}